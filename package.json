{
  "name": "electron-microscope",
  "version": "1.0.0-beta1",
  "license": "MIT",
  "angular-cli": {},
  "main": "dist/entry.js",
  "scripts": {
    "clean": "rm -rf dist",
    "postinstall": "cpx webpack-build-common.js node_modules/angular-cli/models && cpx webpack-build-test.js node_modules/angular-cli/models",
    "run": "electron .",
    "start": "concurrently \"ng serve\" \"electron src/entry.live.js\"",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "package:linux": "electron-packager . electron-microscope-$npm_package_version --out=packages --platform=linux --arch=all --overwrite --icon=icons/icon.ico",
    "package:mac": "electron-packager . electron-microscope-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite --icon=icons/icon.icns",
    "package:win": "electron-packager . electron-microscope-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite --icon=icons/icon.ico",
    "package:all": "electron-packager . electron-microscope-$npm_package_version --all --overwrite",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "test:phantom": "ng test --browsers PhantomJS",
    "test:ci": "ng test --browsers PhantomJS --single-run",
    "pree2e": "webdriver-manager update",
    "pre-push": "npm run lint && npm run test:ci",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/material": "2.0.0-alpha.11",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "angular2-data-table": "0.12.0",
    "angular2-highcharts": "0.3.5",
    "color-stepper": "1.0.0-beta",
    "core-js": "^2.4.1",
    "current-processes": "0.2.1",
    "dateformat": "2.0.0",
    "electron": "1.4.1",
    "ps-node": "^0.1.4",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/dateformat": "^1.0.1",
    "@types/electron": "^1.4.31",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "1.0.0-beta.1",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "electron": "^1.4.1",
    "electron-packager": "^8.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
