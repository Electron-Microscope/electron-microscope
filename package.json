{
  "name": "electron-microscope",
  "version": "1.0.0-beta1",
  "license": "MIT",
  "angular-cli": {},
  "main": "dist/entry.js",
  "scripts": {
    "run": "electron .",
    "package:linux": "electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=node_modules --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=linux --arch=all --overwrite",
    "package:mac": "electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=node_modules --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=darwin --arch=all --overwrite ",
    "package:win": "electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=node_modules --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=win32 --arch=all --overwrite ",
    "package:all": "electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=node_modules --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --all --overwrite",
    "test:ci": "xvfb-run -e /dev/stdout -a karma start ./karma.conf.js --single-run",
    "pre-push": "npm run lint && npm run test:ci",
    "build": "webpack",
    "lint": "ng lint --type-check",
    "lint:fix": "ng lint --type-check --fix",
    "start": "concurrently \"webpack-dev-server --port=4200\" \"electron src/entry.live.js\"",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@swimlane/ngx-datatable": "^9.3.0",
    "angular2-highcharts": "0.3.5",
    "color-stepper": "1.0.0-beta",
    "core-js": "^2.4.1",
    "current-processes": "0.2.1",
    "dateformat": "2.0.0",
    "ps-node": "^0.1.4",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler-cli": "^4.0.0",
    "@ngtools/webpack": "^1.4.1",
    "@types/dateformat": "^1.0.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "devtron": "^1.4.0",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-loader": "^0.5.4",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-electron": "^5.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^7.0.0",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "~2.0.0",
    "tslint": "^5.0.0",
    "typescript": "2.2.0",
    "url-loader": "^0.5.7",
    "webdriver-manager": "10.2.5",
    "webpack": "^2.6.1",
    "webpack-dev-server": "~2.4.5"
  }
}
