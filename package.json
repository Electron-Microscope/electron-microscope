{
  "name": "electron-microscope",
  "version": "0.0.1-beta8",
  "license": "MIT",
  "angular-cli": {},
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "postinstall": "cp ./webpack-build-common.js ./node_modules/angular-cli/models/webpack-build-common.js",
    "start": "electron .",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "package:linux": "electron-packager . electron-microscope-$npm_package_version --out=packages --platform=linux --arch=all --overwrite --icon=icons/icon.ico",
    "package:mac": "electron-packager . electron-microscope-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite --icon=icons/icon.icns",
    "package:win": "electron-packager . electron-microscope-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite --icon=icons/icon.ico",
    "package:all": "electron-packager . electron-microscope-$npm_package_version --all --overwrite",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "test:phantom": "ng test --browsers PhantomJS",
    "test:ci": "ng test --browsers PhantomJS --single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "pre-push": "npm run lint && npm run test:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/material": "2.0.0-alpha.9-3",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-data-table": "0.10.1",
    "angular2-highcharts": "0.3.4",
    "color-stepper": "1.0.0-beta",
    "core-js": "2.4.1",
    "current-processes": "0.2.1",
    "dateformat": "1.0.12",
    "electron": "1.4.1",
    "ps-node": "^0.1.4",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "1.1.1",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "2.2.30",
    "angular-cli": "1.0.0-beta.15",
    "codelyzer": "0.0.26",
    "electron-packager": "^8.2.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "phantomjs-prebuilt": "2.1.12",
    "protractor": "4.0.5",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2"
  }
}
